@model  Condominio

<div class="form-group">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="Blocos"></label>
    <input asp-for="Blocos" class="form-control" />
</div>
<div class="form-group">
    <input asp-for="Ativo" />
    <label asp-for="Ativo"></label>
</div>
<div class="form-group">
    <label asp-for="Tipo"></label>
    <select asp-for="Tipo" class="form-control"
            asp-items="@Html.GetEnumSelectList<Tipo>()">
        <option>Selecione</option>
    </select>
</div>

<fieldset>
    <legend>Sindico</legend>
    <div class="form-group">
        <label asp-for="Sindico.Nome"></label>
        <input asp-for="Sindico.Nome" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Sindico.Email"></label>
        <input asp-for="Sindico.Email" class="form-control" />
    </div>
</fieldset>
